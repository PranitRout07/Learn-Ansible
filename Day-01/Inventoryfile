    web01:
      ansible_host: <Private-IP>
      ansible_user: ec2-user
      ansible_ssh_private_key_file: <server-key>.pem
    web02:
      ansible_host: <Private-IP>
      ansible_user: ec2-user
      ansible_ssh_private_key_file: <server-key>.pem
    db01:
      ansible_host: <Private-IP>
      ansible_user: ec2-user
      ansible_ssh_private_key_file: <server-key>.pem

